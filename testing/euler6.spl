# Solves Project Euler 6

def sumOfSquares n :: Int -> Int =
	sum := 0
	i := 1
	while i <= n do
		sum = sum + i * i
		i = i + 1

	return sum

def squareOfSum n :: Int -> Int =
	sum := 0
	i := 1
	while i <= n do
		sum = sum + i
		i = i + 1

	return sum * sum

print $ squareOfSum 100 - sumOfSquares 100

