# Solves Project Euler 6

def sum_of_squares n :: Int -> Int =
	let sum :: Int = 0
	let i :: Int = 1
	while i <= n do
		sum = sum + i * i
		i = i + 1

	return sum

def square_of_sum n :: Int -> Int =
	let sum :: Int = 0
	let i :: Int = 1
	while i <= n do
		sum = sum + i
		i = i + 1

	return sum * sum

print square_of_sum 100 - sum_of_squares 100

