# Solves Project Euler 10

def crossOff (p: Int, xs: [Int]) -> Int 
    if null xs:
        return []
    else:
        x := head xs
        if x mod p != 0:
            return x :: crossOff p (tail xs)
        else:
            return crossOff p (tail xs)

def sieve (ys: [Int]) -> [Int] 
    p := head ys
    xs := tail ys
    print $ show p
    zs := crossOff p xs

    return p :: sieve zs

print $ show $ sum $ sieve $ range 2 2000000

