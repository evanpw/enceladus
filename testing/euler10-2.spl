-- Solves Project Euler 10

def crossOff p xs :: Int -> List -> List =
    if null xs then
        return []
    else
        let x :: Int = head xs
        if x mod p != 0 then
            return x : crossOff p (tail xs)
        else
            return crossOff p (tail xs)

def sieve ys :: List -> List =
    let p :: Int = head ys
    let xs :: List = tail ys
    print $ show p
    let zs :: List = crossOff p xs

    return p : sieve zs

print $ show $ sum $ sieve $ range 2 2000000

