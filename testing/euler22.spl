# Solves Project Euler #22
import IO
import List
import String

def readNames() -> [String]
    line := readLine(stdin)

    pieces := line.unwrap().splitBy(',')

    result := []
    for piece in pieces
        result = result.insert(piece.stripChar('"'))

    return result

def score(name: String) -> Int
    total := 0
    for i in 0 to name.length()
        c := name.at(i)
        total += (c - 'A') as Int + 1

    return total

names := readNames()
names = sort(names)

total := 0
for i in 0 to names.length()
    name := names.at(i)

    total += ((i + 1) as Int) * score(name)

println $ show(total)
