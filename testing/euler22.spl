# Solves Project Euler #22

import List
import String

def readNames() -> [String]
    line := readLine()

    pieces := line.unwrap().splitBy(',')

    result := []
    for piece in pieces
        result = piece.trimChar('"') :: result

    return result

def score(name: String) -> Int
    total := 0
    for i in range(0, name.length())
        c := name.at(i)
        total += (c - 'A') + 1

    return total

names := readNames()
names = sort(names)

total := 0
for i in range(0, names.length())
    name := names.at(i)

    total += ((i + 1) as Int) * score(name)

print $ show(total)
