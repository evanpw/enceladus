-- Solves Project Euler #22

import List
import String

def readNames () -> [String] =
    line := readLine

    pieces := splitBy ',' line

    result := []
    for piece in pieces do
        result = (trimChar '"' piece) : result

    return result

def score (name: String) -> Int =
    total := 0
    for c in name do
        total += (c - 'A') + 1

    return total

names := readNames
names = sort cmpList names

total := 0
for i in range 1 (length names) do
    name := at (i - 1) names

    total += i * score name

print $ show total
