# Solves Project Euler 7

def isPrime x :: Int -> Bool =
    let i :: Int = 2
    while i * i <= x do
        if x mod i == 0 then return False
        i += 1

    return True

let x :: Int = 2
let n :: Int = 0
while n < 10001 do
    if isPrime x then n += 1

    if n == 10001 then
        print x
    else
        x += 1
