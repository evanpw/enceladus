# Solve Project Euler 4

def reverseDigits(x: Int) -> Int
    y := 0

    while x != 0
        y = y * 10 + (x % 10)
        x /= 10

    return y

biggest := 0

for i in range(100, 2000)
    for j in range(1, 10000)
        x := i * j

        if x == reverseDigits(x) and x > biggest
            biggest = x

println $ show(biggest)
