# Solve Project Euler 4

def reverseDigits(x: Int) -> Int
    y := 0

    while x != 0
        y = y * 10 + (x % 10)
        x /= 10

    return y

biggest := 0

for i in 100 to 2000
    for j in 1 to 10000
        x := i * j

        if x == reverseDigits(x) and x > biggest
            biggest = x

println $ show(biggest)
