def add<T: Num>(x: T, y: T) -> T
    return x + y

print $ show $ add(1, 2)
print $ showUInt $ add(3, 4)

data Nothing = Nothing

impl Nothing
    def addTwoNumbers<T: Num>(self: Nothing, lhs: T, rhs: T) -> T
        return lhs + rhs

x := Nothing
print $ show $ x.addTwoNumbers(5, 6)
print $ showUInt $ x.addTwoNumbers(7, 8)
