def add(x: T, y: T) -> T where T: Num
    return x + y

println $ show $ add(1, 2)
println $ showUInt $ add(3, 4)

data Nothing = Nothing

impl Nothing
    def addTwoNumbers(self, lhs: T, rhs: T) -> T where T: Num
        return lhs + rhs

x := Nothing
println $ show $ x.addTwoNumbers(5, 6)
println $ showUInt $ x.addTwoNumbers(7, 8)
