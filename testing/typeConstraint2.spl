def add(x: T, y: T) -> T where T: Num
    return x + y

print $ show $ add(1, 2)
print $ showUInt $ add(3, 4)

data Nothing = Nothing

impl Nothing
    def addTwoNumbers(self: Nothing, lhs: T, rhs: T) -> T where T: Num
        return lhs + rhs

x := Nothing
print $ show $ x.addTwoNumbers(5, 6)
print $ showUInt $ x.addTwoNumbers(7, 8)
