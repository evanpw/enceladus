-- Solves Project Euler 10

def is_prime x primes :: Int -> List -> Bool =
    for p in primes do
        if x mod p == 0 then return False
        if p * p > x then return True

    return True

def primes_below n :: Int -> List =
    let primes :: List = []

    for i in range 2 n do
        if is_prime i (reverse primes) then
            primes = i : primes

    return primes

print sum $ primes_below 2000000
