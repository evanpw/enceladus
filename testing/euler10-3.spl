# Solves Project Euler 10

def isPrime (x: Int, primes: [Int]) -> Bool 
    for p in primes
        if x mod p == 0
            return False

        if p * p > x
            return True

    return True

def primesBelow (n: Int) -> [Int] 
    primes := []

    for i in range 2 n
        if isPrime i (reverse primes)
            primes = i :: primes

    return primes

print $ show $ sum $ primesBelow 2000000
