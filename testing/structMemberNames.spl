struct Person =
    name: String
    age: Int

struct Location = 
    name: String
    country: String

def getAge(person: Person) -> Int =
    let Person _ age = person
    return age

def getCountry(location: Location) -> String =
    let Location _ country = location
    return country

print $ show $ getAge (Person "Evan" 29)
print $ getCountry (Location "Eiffel Tower" "France")
