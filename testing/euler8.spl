def length xs :: List -> Int =
    if null xs then
        return 0
    else
        return 1 + length $ tail xs

def sum xs :: List -> Int =
    if null xs then
        return 0
    else return head xs + sum $ tail xs

let xs :: List = 1 : 2 : 3 : 4 : 5 : []
print length xs
print sum xs
