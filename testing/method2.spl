impl Int
    def eq(self: Int, other: Int) -> Bool
        return self == other

impl [Int]
    def eq(self: [Int], other: [Int]) -> Bool
        if self.null()
            return other.null()

        elif other.null()
            return False

        else
            return self.head() == other.head() and self.tail().eq(other.tail())

x := [1, 2]
y := [1, 2]

if x.eq(y)
    print $ "Same"
else
    print $ "Different"
