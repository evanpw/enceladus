impl Int
    def equal(self: Int, other: Int) -> Bool
        return self == other

impl [Int]
    def equal(self: [Int], other: [Int]) -> Bool
        if self.empty()
            return other.empty()

        elif other.empty()
            return False

        else
            return self.head() == other.head() and self.tail().equal(other.tail())

x := [1, 2]
y := [1, 2]

if x.equal(y)
    print $ "Same"
else
    print $ "Different"
