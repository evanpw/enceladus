# Approximate the square root of 2 (actually a multiple of it, since we have to use integer arithmetic)

N <- 20000 	# We'll find the square root of 2 * 10^4, which will be sqrt(2) * 10^2 = 142

# Newton's method, with starting guess 1
x <- 1
i <- 0
loop:
	x <- x - (x * x - N) / (2 * x)
	print $ show x
	
	i <- i + 1
	if not (i > 100) then goto loop
	
print $ show x
