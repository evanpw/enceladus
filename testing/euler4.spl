# Solve Project Euler 4

def reverseDigits(x: Int) -> Int
    y := 0

    while x != 0
        y = y * 10 + (x % 10)
        x /= 10

    return y

biggest := 0

for i in 100 to 999
    for j in i + 1 to 1000

        x := i * j

        if x > biggest and x == reverseDigits(x)
            biggest = x

println $ show(biggest)
