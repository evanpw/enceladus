# Solve Project Euler 4

let biggest :: Int = 0

let i :: Int = 100
while i <= 999 do
    let j :: Int = 100
    while j <= 999 do
        let x :: Int = i * j

        let copy_x :: Int = x
        let y :: Int = 0

        while copy_x != 0 do
            y = y * 10
            y = y + (copy_x mod 10)
            copy_x = copy_x / 10

        if x == y and x > biggest then
            biggest = x

        j = j + 1

    i = i + 1

print biggest
