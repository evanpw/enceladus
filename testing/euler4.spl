# Solve Project Euler 4

let biggest :: Int = 0

let i :: Int = 100
while i <= 999 do
    let j :: Int = 100
    while j <= 999 do
        let x :: Int = i * j

        let copyX :: Int = x
        let y :: Int = 0

        while copyX != 0 do
            y = y * 10
            y = y + (copyX mod 10)
            copyX = copyX / 10

        if x == y and x > biggest then
            biggest = x

        j = j + 1

    i = i + 1

print biggest
