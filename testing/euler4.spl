-- Solve Project Euler 4

def reverseDigits (x: Int) -> Int =
    y := 0

    while x != 0 do
        y = y * 10 + (x mod 10)
        x /= 10

    return y

biggest := 0

for i in range 100 999 do
    for j in range 1 999 do
        x := i * j

        if x == reverseDigits x and x > biggest then
            biggest = x

print $ show biggest
