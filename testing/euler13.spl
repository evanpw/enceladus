# Solves Project Euler 13

import BigInt

def readNumber() -> BigInt
    ys := []

    for j = 1 to 50
        ys = read :: ys

    return BigInt(ys)

xs := toBigInt(0)
for i = 1 to 100
    xs = xs.plus(readNumber())

n := 0
for x in take(10, reverse(xs.digits()))
    n = (n * 10) + x

print $ show(n)
