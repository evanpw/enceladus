# Solves Project Euler 13

import BigInt

def readNumber() -> BigInt
    ys := []

    for j in range(0, 50)
        ys = ys.insert(read)

    return BigInt(ys)

xs := toBigInt(0)
for i in range(0, 100)
    xs = xs.plus(readNumber())

n := 0
for x in xs.digits().reverse().take(10)
    n = (n * 10) + x

print $ show(n)
