# Solves Project Euler #21

def divisors(n: Int) -> [Int]
    result := [1]

    i := 2
    while i * i <= n
        if n % i == 0
            if i * i == n
                result = result.insert(i)
            else
                result = result.insert(n / i).insert(i)

        i += 1

    return result

def isAmicable(n: Int) -> Bool
    n' := sum $ divisors(n)

    if n == n'
        return False

    n'' := sum $ divisors(n')

    return n == n''

total := 0
for n in range(1, 10000)
    if isAmicable(n)
        total += n

println $ show(total)

