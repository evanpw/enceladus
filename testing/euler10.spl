# Solves Project Euler 10
primeSum := 0
n := 2000000

maybePrime := makeArray(n, True)

primes := []
for i = 2 to n - 1
    if maybePrime.[i]
        # i is a prime
        primeSum += i

        # All multiples of i are not prime (start at i^2 since everything
        # below has already been eliminated)
        j := i * i
        while j < n
            arraySet(maybePrime, j, False)
            j += i

print $ show(primeSum)
