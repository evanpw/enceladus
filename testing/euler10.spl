-- Solves Project Euler 10

def isPrime x :: Int -> Bool =
    let i :: Int = 2
    while i * i <= x do
        if x mod i == 0 then return False
        i += 1

    return True

def primesBelow n :: Int -> List =
    let primes :: List = []

    for i in range 2 n do
        if isPrime i then
            primes = i : primes

    return primes

print sum $ primesBelow 2000000
