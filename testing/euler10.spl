-- Solves Project Euler 10

def is_prime x :: Int -> Bool =
    let i :: Int = 2
    while i * i <= x do
        if x mod i == 0 then return False
        i += 1

    return True

def primes_below n :: Int -> List =
    let primes :: List = []

    for i in range 2 n do
        if is_prime i then
            primes = i : primes

    return primes

print sum $ primes_below 2000000
