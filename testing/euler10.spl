-- Solves Project Euler 10

def isPrime x :: Int -> Bool =
    let i :: Int = 2
    while i * i <= x do
        if x mod i == 0 then return False
        i += 1

    return True

def sumPrimes n :: Int -> Int =
    let total :: Int = 0

    for i in range 2 n do
        if isPrime i then total += i

    return total

print $ sumPrimes 2000000
