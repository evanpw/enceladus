-- Solves Project Euler 10

def isPrime(x: Int) -> Bool 
    i := 2
    while i * i <= x
        if x mod i == 0
            return False

        i += 1

    return True

def sumPrimes(n: Int) -> Int 
    total := 0

    for i in range 2 n
        if isPrime i
            total += i

    return total

print $ show $ sumPrimes 2000000
