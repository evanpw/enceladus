# Tests of local optimizations

def computeValue() -> Int
    x := 4

    y := x * 1
    z := x * 0
    w := x + 0
    q := x - 0

    a := 1 + 3
    b := 1 * 3
    c := 1 / 3
    d := 1 - 3
    e := 1 % 3

    return x + y + z + w + a + b + c + d + e + q

result := 0
for i in range(0, 10000000)
    result = computeValue()

print $ show(result)
