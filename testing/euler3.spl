# Solves Project Euler 3

n := 600851475143
biggest := 0

# Loop over all possible divisors of n
i := 2
while i * i <= n do

	# When we find a divisor, determine whether it is a prime number
	if n mod i == 0 then
		j := 2
		isPrime := 1
		while j * j <= i do
			if i mod j == 0 then
				isPrime  = 0

				# We don't have a "break" statement
				j = i

			j = j + 1

		if isPrime == 1 then
			biggest = i

	i = i + 1

print biggest
