-- Solves Project Euler 3

def isPrime (x: Int) -> Bool =
    i := 2
    while i * i <= x do
        if x mod i == 0 then return False
        i += 1

    return True

n := 600851475143
biggest := 0

-- Loop over all possible divisors of n
i := 2
while i * i <= n do

	if n mod i == 0 and isPrime i then biggest = i

	i += 1

print $ show biggest
