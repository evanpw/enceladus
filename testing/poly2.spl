def mydrop n xs =
    if n == 0 then
        return xs
    else
        return mydrop (n - 1) (tail xs)

let xs = 1 : 2 : 3 : 4 : []
let ys = mydrop 2 xs
let n = head ys

let zs = True : True : True : False : False : False : []
let us = mydrop n zs
if head us then
   echo "True"
else
   echo "False"
