def getOneFifty() -> UInt8
    return 150u8

def getTen() -> UInt8
    return 10u8

oneFifty := getOneFifty()
ten := getTen()

result := oneFifty + oneFifty
println $ showUInt(result as UInt)

result = ten - oneFifty
println $ showUInt(result as UInt)

result = ten * oneFifty
println $ showUInt(result as UInt)

result = oneFifty / ten
println $ showUInt(result as UInt)

result = oneFifty % ten
println $ showUInt(result as UInt)
